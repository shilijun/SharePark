/* miniprogram/pages/findParking/findParking.wxss */

page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}


.section{
  margin-top: 40rpx;
  background: #fff;
  border: 1px solid rgba(0, 0, 0, 0.1);
  display: flex;
  flex-direction: column;
  align-items: center;
  /* 上右下左 */
  padding: 30rpx 0 0 40rpx;  
  flex-direction: column;
  align-items: flex-start;
  box-sizing: border-box;
}
.item{
  /* 上，左右，下 */
  padding: 10rpx 0 10rpx; 
  display: flex;
  align-items: center;
}
.item-title{
  color: #aaa;
  margin-right: 30rpx;
}